<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "Day_4.beans.Restaurant" table="restaurant_master_details">
      <id name = "restaurantId" type = "int" column="rest_Id"/>            
      <property name = "name" column = "rest_name" type = "string"/>
      <property name = "cuisine" column = "rest_cuisine" type = "string"/>
      <property name = "branchCOunt" type = "int">
      	<column name="rest_Branches"></column>
      </property>     
   </class>
</hibernate-mapping>